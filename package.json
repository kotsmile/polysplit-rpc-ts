{
  "engine": {
    "node": "16"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "dev": "tsup --watch --onSuccess \"yarn serve\"",
    "build": "yarn lint && yarn type && tsup",
    "serve": "node dist/index.js",
    "type": "tsc --noEmit",
    "lint": "prettier --check ./src/**/* && eslint ./src",
    "format": "prettier --write ./src/**/*",
    "do": "./shell/script.sh",
    "gen-client": "yarn openapi-zod-client ./openapi.yml -o ../frontend/api/client.ts",
    "gen-doc": "yarn do gen-doc && yarn gen-swagger-page",
    "gen-swagger-page": "node ./node_modules/swagger-to-static/index.js ./openapi.yml ./swagger"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "openapi-zod-client": "^1.10.2",
    "prettier": "^2.8.8",
    "swagger-to-static": "^1.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cron": "^2.4.3",
    "ethers": "^6.10.0",
    "express": "^4.18.2",
    "express-zod-api": "^11.3.0",
    "https-proxy-agent": "^7.0.2",
    "mongodb": "^6.3.0",
    "node-cache": "^5.1.2",
    "ts-results": "^3.3.0",
    "tslog": "^4.8.2",
    "zennv": "^0.1.1",
    "zod": "^3.21.4",
    "zodios": "^5.1.0"
  }
}
