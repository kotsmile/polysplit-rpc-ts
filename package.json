{
  "engine": {
    "node": "18"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "postinstall": "prisma generate && prettier --write ./src/services/db/zod/*",
    "dev": "tsup --watch --onSuccess \"yarn serve\"",
    "build": "yarn lint && yarn type && tsup",
    "serve": "yarn gen-doc && yarn prisma:apply-migration && node dist/index.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "type": "tsc --noEmit",
    "lint": "prettier --check ./src/**/* && eslint ./src",
    "format": "prettier --write ./src/**/*",
    "do": "./shell/script.sh",
    "prisma:create-migration": "yarn prisma migrate dev --create-only",
    "prisma:apply-migration": "yarn prisma migrate deploy",
    "gen-client": "yarn openapi-zod-client ./openapi.yml -o ../frontend/api/client.ts",
    "gen-doc": "yarn do gen-doc && yarn gen-swagger-page",
    "gen-swagger-page": "node ./node_modules/swagger-to-static/index.js ./openapi.yml ./swagger"
  },
  "devDependencies": {
    "@jest/types": "^29.6.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "chai": "^4.3.7",
    "eslint": "^8.46.0",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "openapi-zod-client": "^1.10.2",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "swagger-to-static": "^1.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "zod-prisma": "^0.5.4"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "@zodios/core": "^10.9.2",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cron": "^2.4.3",
    "discord.js": "^14.13.0",
    "express": "^4.18.2",
    "express-zod-api": "^11.3.0",
    "googleapis": "^126.0.1",
    "jsonwebtoken": "^9.0.1",
    "ms": "^2.1.3",
    "puppeteer": "^21.1.0",
    "superjson": "^1.12.4",
    "telegraf": "^4.12.2",
    "ts-results": "^3.3.0",
    "tslog": "^4.8.2",
    "zennv": "^0.1.1",
    "zod": "^3.21.4",
    "zodios": "^5.1.0"
  }
}
