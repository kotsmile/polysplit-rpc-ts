{
  "name": "polysplit-rpc",
  "module": "src/index.js",
  "type": "module",
  "scripts": {
    "postinstall": "bunx prisma generate && prettier --write ./src/services/storage/zod/*",
    "dev": "bun run --watch ./src/index.ts",
    "build": "bun run lint && bun run ./build.ts",
    "serve": "bun run dist/index.js",
    "serve:db": "bun prisma:apply-migration && node dist/index.js",
    "lint": "prettier --check ./src/**/* && eslint ./src",
    "format": "prettier --write ./src/**/*",
    "prisma:create-migration": "bunx prisma migrate dev --create-only",
    "prisma:apply-migration": "bunx prisma migrate deploy"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "bun-types": "latest",
    "eslint": "^8.46.0",
    "prettier": "^2.8.8",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "zod-prisma": "^0.5.4"
  },
  "dependencies": {
    "@elysiajs/cron": "^0.8.0",
    "@prisma/client": "^5.8.0",
    "axios": "^1.4.0",
    "elysia": "^0.8.8",
    "ethers": "6",
    "node-cache": "^5.1.2",
    "ts-results": "^3.3.0",
    "tslog": "^4.8.2",
    "zod": "^3.21.4"
  }
}
